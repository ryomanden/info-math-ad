package activity;

public class number1 {
	public static void main(String[] args) {
		int a[][] = {
				{1,2,3},
				{4,5,6},
				{7,8,9}
		};
		
		int b[][] = {
				{1,0,0},
				{0,2,0},
				{0,0,3}
		};
		
		int[][] ans = new int[3][3];
		
		for(int i = 0; i < a.length; i++) {
			for(int j = 0; j < a[i].length; j++) {
				ans[i][j] = a[i][j] + b[j][i];
			}
		}
		for(int i = 0; i < ans.length; i++) {
			for(int j = 0; j < ans[i].length; j++) {
				System.out.print(ans[i][j]);
			}
			System.out.println();
		}
		
	}

}
